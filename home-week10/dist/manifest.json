{
    "manifest_version": 3,
    "name": "Pomodoro Timer",
    "version": "0.1.0",
    "host_permissions": ["<all_urls>"],
    "permissions": [
    "notifications"
  ],
    "background": {
    "service_worker": "background.js"
    },
    "action": {
    "default_popup": "index.html"
    }
    }

    